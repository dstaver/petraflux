{
  "name": "@petraflux/react-virtual",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsgo",
    "check": "prettier --write . && oxlint --fix",
    "dev": "vite --port 3000",
    "format": "prettier --check --write .",
    "lint": "oxlint -f stylish",
    "lint-fix": "oxlint -f stylish --fix",
    "serve": "vite preview",
    "test": "vitest run",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "oxlint": "catalog:",
    "vite": "catalog:"
  }
}
